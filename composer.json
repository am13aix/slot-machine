{
    "name": "Slot Machine",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["PHP", "Lumen", "slot-machine"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9",
        "ext-json": "*",
        "ext-http": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Test\\": "tests/",
            "Demo\\": "src/Demo"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
